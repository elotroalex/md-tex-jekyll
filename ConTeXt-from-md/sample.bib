@article{gentner_alignment_1997,
	title = {Alignment in the {Processing} of {Metaphor}},
	volume = {37},
	issn = {0749-596X},
	url = {http://www.sciencedirect.com/science/article/pii/S0749596X97925274},
	doi = {10.1006/jmla.1997.2527},
	abstract = {This research investigates the process of metaphor comprehension. We compare two classes of processing algorithms: those that begin by accessing or deriving an abstraction from the base (orvehicle) and then project it to the target (ortopic) and those that begin by aligning the representations of the terms and then project further inferences from the base to the target. To decide between these accounts, we recorded subjects’ time to interpret metaphors primed by either the base term or the target term (or both or neither). Abstraction-based models predict that priming by the base should lead to faster metaphor comprehension than priming by the target; alignment-based models predict no such advantage. Across a series of experiments, the results were most consistent with alignment-first processing. No base advantage was found, with the single exception of metaphors having highly conventional meanings and low (metaphorical) similarity. Further, high-similarity metaphors were interpreted faster than low-similarity metaphors, consistent with the alignment view. We conjecture that this pattern may result from a shift in processing with conventionalization: novel metaphors may be understood by alignment and conventional metaphors by abstraction.},
	number = {3},
	urldate = {2015-12-06},
	journal = {Journal of Memory and Language},
	author = {Gentner, Dedre and Wolff, Phillip},
	month = oct,
	year = {1997},
	pages = {331--355},
	file = {ScienceDirect Full Text PDF:/home/denten/.mozilla/firefox/ratooqzd.default/zotero/storage/EDFZZUIS/Gentner and Wolff - 1997 - Alignment in the Processing of Metaphor.pdf:application/pdf;ScienceDirect Snapshot:/home/denten/.mozilla/firefox/ratooqzd.default/zotero/storage/ZWGHUEZ6/S0749596X97925274.html:text/html}
